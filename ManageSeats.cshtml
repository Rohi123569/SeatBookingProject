<h2>Manage Seats</h2>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Seat ID</th>
            <th>Type</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var seat in Model)
        {
            <tr>
                <td>@seat.Id</td>
                <td>@seat.Type</td>
                <td>@(seat.IsBooked ? "Booked" : "Available")</td>
            </tr>
        }
    </tbody>
</table>