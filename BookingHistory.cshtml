<h2>Your Booking History</h2>
<table class="table">
    <thead>
        <tr>
            <th>Booking ID</th>
            <th>Seat</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var booking in Model)
        {
            <tr>
                <td>@booking.Id</td>
                <td>@booking.Seat.Id</td>
                <td>@booking.BookingDate.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>